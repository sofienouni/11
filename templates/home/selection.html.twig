{% extends 'base.html.twig' %}

{% block title %}11.tn!{% endblock %}

{% block body %}

    <section class="form-12" id="home">
        {{ form_start(form) }}
        <div class="form-12-content">
            <div class="container">
                <div class="grid grid-column-2 ">
                    <div class="column2">
                    </div>
                    <div class="column1">
                        <div class="blog-search form d-flex search-form">
                            {{ form_row(form.ref) }}
                        </div>
                            <div class="form-top">
                                {{ form_row(form.type) }}
                                {{ form_row(form.typeBien) }}
                            </div>
                            <div class="form-top">
                                {{ form_row(form.ville) }}
                                {{ form_row(form.prix) }}
                            </div>
                            <div class="form-top">

                            </div>
                            <button type="submit" class="btn">Rechercher</button>
                    </div>

                </div>
            </div>
        </div>
        {{ form_end(form) }}
    </section>


