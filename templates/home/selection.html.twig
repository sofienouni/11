    <script type="application/javascript">
        $('.nav-link').on('click', function(){
            console.log($(this).data('bsTarget'));
        $('.content ').removeClass('active');
        $('.content ').removeClass('show');
        $($(this).data('bsTarget')).addClass('active');
        $($(this).data('bsTarget')).addClass('show');
        });

        $('#vente-tab').on('click', function(){
            $('#appart').addClass('active');
            $('#location').removeClass('active');
            $('#location').removeClass('show');
            $('.nav-link ').removeClass('active');
            $('#appart-tab').addClass('active');
            $('#appart').addClass('show');

        });
        $('#location-tab').on('click', function(){
            $('#lappart').addClass('active');
            $('#vente').removeClass('active');
            $('#vente').removeClass('show');
            $('.nav-link ').removeClass('active');
            $('#lappart-tab').addClass('active');
            $('#lappart').addClass('show');
        });
    </script>
    <script type="application/javascript" async defer>
        $(document).on("click",'.save_selection',function () {
            $(this).removeClass();
            $(this).addClass('btn btn-danger remove_selection');
            var id = $(this).val();
            $.ajax({
                type: 'GET',
                url: '/save_selection/'+id,
                dataType: 'json',
                encode: true
            })
                .done(function(data) {
                    console.log(data);
                })
        });
        $(document).on("click",'.remove_selection',function () {
            $(this).removeClass();
            $(this).addClass('btn btn-secondary save_selection');
            var id = $(this).val();
            $.ajax({
                type: 'GET',
                url: '/remove_selection/'+id,
                dataType: 'json',
                encode: true
            })
                .done(function(data) {
                    console.log(data);
                })
        });

    </script>
